
public class Cat extends Animals {

	public String name;
	public String type;

	public Cat(int age, double weigt, boolean sex, String ration, String name, String type) {
		super(age, weigt, sex, ration);
		this.name = name;
		this.type = type;
	}

	public Cat() {
		super();
	}
	

	@Override
	public void getVoice() {
		System.out.println("Meow, Meow");
		
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	@Override
	public String toString() {
		return "Cat [name=" + name + ", type=" + type + " ] ";
	}
		
//	@Override
//	public String toString() {
//		return "Cat [getAge()=" + getAge() + ", getWeigt()=" + getWeigt() + ", isSex()=" + isSex() + ", getRation()="
//				+ getRation() + ", getName()=" + getName() + ", getType()=" + getType() + "]";
//	}

}
